{
  "name": "localistars-landing",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "404": "cp out/404/index.html out/404.html",
    "dev": "next dev",
    "build": "NEXT_TELEMETRY_DISABLED=1 next build && NEXT_TELEMETRY_DISABLED=1 next export",
    "postbuild": "next-sitemap --config next-sitemap.config.js && npm run ensureSitemap && npm run 404",
    "ensureSitemap": "[ -f out/sitemap.xml ] || cp public/sitemap*.xml out/",
    "server": "next start",
    "start": "npm run dev",
    "lint": "next lint",
    "serve": "http-server out -p 8080",
    "pretty": "prettier --write ."
  },
  "dependencies": {
    "@n8tb1t/use-scroll-position": "^2.0.3",
    "babel-eslint": "^10.1.0",
    "bootstrap": "^5.1.3",
    "esbuild": "0.14.23",
    "eslint-import-resolver-typescript": "^2.5.0",
    "framer-motion": "^4.1.17",
    "i18next-browser-languagedetector": "6.1.3",
    "mdx-bundler": "8.0.1",
    "next": "12.1.0",
    "next-i18next": "10.5.0",
    "next-images": "^1.8.4",
    "next-language-detector": "1.0.2",
    "next-sitemap": "^2.5.4",
    "polished": "^4.1.4",
    "react": "17.0.2",
    "react-bootstrap": "^1.6.4",
    "react-cookie-consent": "7.3.1",
    "react-countup": "^6.1.1",
    "react-dom": "17.0.2",
    "react-ga": "3.3.0",
    "react-icons": "^4.3.1",
    "react-is": "^17.0.2",
    "react-lazyload": "^3.2.0",
    "react-masonry-css": "^1.0.16",
    "react-modal-image": "^2.5.0",
    "react-modal-video": "^1.2.8",
    "react-scroll-to-top": "^1.0.8",
    "react-slick": "^0.28.1",
    "react-typing-effect": "^2.0.5",
    "reading-time": "1.5.0",
    "rehype-autolink-headings": "6.1.1",
    "rehype-code-titles": "1.0.3",
    "rehype-prism-plus": "1.3.2",
    "rehype-slug": "5.0.1",
    "remark-gfm": "3.0.1",
    "sass": "^1.49.9",
    "slick-carousel": "^1.8.1",
    "styled-components": "^5.3.3",
    "url-loader": "^4.1.1"
  },
  "devDependencies": {
    "babel-plugin-styled-components": "^2.0.6",
    "eslint": "7.32.0",
    "eslint-config-next": "12.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "gray-matter": "^4.0.3",
    "http-server": "14.1.0",
    "next-transpile-modules": "9.0.0",
    "prettier": "2.5.1"
  },
  "prettier": {
    "arrowParens": "always",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "none"
  }
}
